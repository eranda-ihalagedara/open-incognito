{
  "name": "OpenIncognito",
  "action": {},
  "manifest_version": 3,
  "version": "1.0",
  "description": "Opens the URL of the current tab in a new Incognito window",
  "permissions": [
    "activeTab",
    "windows"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  }
}
